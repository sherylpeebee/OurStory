  <body ng-app="OurStory" ng-controller="PrimaryCtrl">
    <!--<h1><%= title %></h1>
    <p>Welcome to <%= title %></p> -->
    <div class="navbar-fixed">
  <nav>
    <div class="nav-wrapper">
      <a ui-sref="home" class="brand-logo">OurStory</a>
      <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
      <ul class="right hide-on-med-and-down">
        <li ng-show="userCheck()" >Welcome back, {{authenticatedUser.username}}!</li>
        <li ng-hide="userCheck()" ng-click="login()"><a href="">sign in/sign up</a></li>
        <li><a ui-sref="currentUser({id:id})" ng-show="userCheck()" >your profile</a></li>
        <li><a href="" ng-show="userCheck()">log out</a></li>
      </ul>
      <ul class="side-nav" id="mobile-demo">
        <li ng-if="userCheck()">Welcome back, {{authenticatedUser.full_name}}!</li>
        <li ng-if="userCheck()"><a href="">sign in/sign up</a></li>
        <li><a ui-sref="currentUser({id:id})">your profile</a></li>
        <li><a href="" ng-show="userCheck()">log out</a></li>
      </ul>
    </div>
  </nav>
</div>
